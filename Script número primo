<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>Script primo</title>
</head>

<script>
var num = prompt("Introduce un número entero");
var resultado = primo(num);
alert("El número "+num+" es "+resultado)

function primo(num){
	cont = 0;

	for(i = 1; i <= num; i++){

	if(num % i == 0){
		cont++;
		}
	}

	if(cont==2){
	return "primo";
	}
	else{
	return "par";
	}
}
</script>

