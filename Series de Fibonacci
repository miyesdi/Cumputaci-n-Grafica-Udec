  <script>
      var a = 1;
      var b = 1;
      var c;

      var d= prompt("Serie fibonnaci introduzca n√∫mero entero:");

                document.write("La   Serie es:");
                document.write("<br>");
                document.write(a   + ", ");
                document.write(b   + ", ");
                c   = b + a;
                document.write(c   + ", ");
                for(var i = 3; i <= d-1;   i++){
		       a = b;
		       b = c;
    		       c = b + a;
        document.write(c +   ", ");
        }                                          
